{
  "version": 2,
  "builds":[
   {
       "src":"/backend/server.js",
       "use":"@vercel/node"
   }
  ],
  "routes":[
   {
       "src":"/api/auth/(.*)",
       "dest":"/backend/server.js"
   },
   {
       "src":"/api/submissions/(.*)",
       "dest":"/backend/server.js"
   },
   {
    "src":"/api/courses/(.*)",
    "dest":"/backend/server.js"
}
  ]

}